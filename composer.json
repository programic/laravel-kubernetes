{
    "name": "programic/laravel-kubernetes",
    "description": "Programic Laravel Kubernetes",
    "autoload": {
        "psr-4": {
            "Programic\\LaravelKubernetes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Nick Verschoor",
            "email": "n.verschoor@programic.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/octane": "^2.6",
        "spatie/laravel-prometheus": "^1.2",
        "spatie/laravel-open-telemetry": "^0.0.11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Programic\\LaravelKubernetes\\KubernetesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "tbachert/spi": true
        }
    }
}
